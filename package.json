{
  "name": "try-pwa-standalone",
  "description": "Try PWA for static site",
  "version": "0.1.0",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "homepage": "https://abhijit945.github.io/try-pwa-standalone/",
  "main": "build/index.js",
  "private": true,
  "scripts": {
    "build": "npm run copy:assets && webpack --mode production",
    "clean": "rimraf build",
    "copy:assets": "npm run clean && mkdir build && cp -r public/* build",
    "deploy": "gh-pages -d build",
    "predeploy": "npm run build",
    "start": "npm run copy:assets && webpack-dev-server --open --mode development"
  }
}
